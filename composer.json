{
    "name": "olakunlevpn/laravel-referral",
    "description": "Laravel package for a referral system",
    "keywords": [
        "olakunlevpn",
        "laravel",
        "referral",
        "affiliate",
        "package",
        "laravel-referral",
        "referral-system",
        "user-referral"
    ],
    "homepage": "https://github.com/olakunlevpn/laravel-referral",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olakunlevpn",
            "email": "olakunlevpn@live.com"
        }
    ],



    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/auth": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4|^10.0|^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Olakunlevpn\\LaravelReferral\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olakunlevpn\\LaravelReferral\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Olakunlevpn\\LaravelReferral\\Providers\\ReferralServiceProvider"
            ]
        }
    }
}
